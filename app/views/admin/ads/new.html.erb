<div class="title">Add Banner Ad</div>

<%= form_for(@ad, :url => admin_ads_path, :html => { :class => 'admin_form', :multipart => true }) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div class="actions">
    <%= f.submit "Create", :disable_with => 'Saving...', :class => 'lg_button' %>
  </div>
<% end %>